<!doctype html>
<html>
  <% include partials/head.ejs %>

  <body>
    <div class="container">
      <h1>URLs</h1>

      <table class="table table-hover table-bordered">
        <thead>
          <tr>
            <th>URL</th>
          </tr>
        </thead>
        <tbody>
          <% data.forEach(function(url) { %>
            <tr>
              <td><%- url  %></td>
            </tr>
          <% }); %>
        </tbody>
      </table>
      <a href="/" class="btn btn-info">Back</a>
  </body>

  
</html>